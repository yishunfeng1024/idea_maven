<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN"      
 "http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">
 <!-- namespace指定和哪个Mapper映射器接口对应 -->
<mapper namespace="cn.ysf.mapper.BootUserMapper">
	<!-- 定义SQL语句 -->

    <insert id="save" parameterType="cn.ysf.bean.BootUser">
       insert into boot_user(name,password) values(#{name},#{password})
   </insert>
    
</mapper>











